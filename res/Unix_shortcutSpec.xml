<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>


<shortcuts>

  <programGroup defaultName="Matrex" location="applications"/>


    <shortcut
     name="Matrex"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="yes"
     startup="no"
     target="java"
     commandLine="-jar $INSTALL_PATH/matrex_start.jar"
     workingDirectory="$INSTALL_PATH"
     description="Start Matrex"
     iconFile="$INSTALL_PATH/images/matrex_ico.png"
     iconIndex="0"
     type="Application"
     encoding="UTF-8"
     terminal="false"
     KdeSubstUID="false"
     initialState="normal">   
     <createForPack name="Base"/>     
   </shortcut>
      
   <shortcut
     name="Matrex Uninstaller"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="java"
     commandLine="-jar &quot;$INSTALL_PATH/Uninstaller/uninstaller.jar&quot;"
     
     initialState="noShow"
     iconFile="trashcan_full"
     iconIndex="0" 
     workingDirectory=""
     type="Application"
     encoding="UTF-8"
     description="Matrex uninstaller">
     <createForPack name="Base" />
   </shortcut>
   
   
</shortcuts>
